{
  "name": "vue-wavify",
  "version": "1.2.2",
  "description": "ðŸŒŠ Animated wave component for Vue (using svg)",
  "author": "Sven Westerlaken",
  "license": "MIT",
  "main": "dist/vue-wavify.ssr.js",
  "browser": "dist/vue-wavify.esm.js",
  "module": "dist/vue-wavify.esm.js",
  "unpkg": "dist/vue-wavify.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "bugs": {
    "url": "https://github.com/SvenWesterlaken/vue-wavify/issues",
    "email": "sven.westerlaken@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SvenWesterlaken/vue-wavify"
  },
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "semantic-release": "semantic-release",
    "semantic-release-dry": "semantic-release -d"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "semantic-release": "^17.3.7",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  },
  "release": {
    "branches": [
      "master",
      "main"
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "doc",
          "scope": "README",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "major",
          "release": "major"
        },
        {
          "type": "build",
          "release": "patch"
        }
      ]
    },
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
